{
  "scripts": {
    "build": "npx tsc",
    "start": "node --max-old-space-size=512 dist/api.main.js",
    "start:data-syncer": "node --max-old-space-size=512 dist/data-syncer.main.js",
    "start:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/api.main.ts",
    "start:data-syncer:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/data-syncer.main.ts",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write --ignore-unknown .",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:unit": "npm run test -- --testPathIgnorePatterns=src/prompts/.*.eval.spec.ts",
    "test:evals": "npm run test -- --testPathPattern=src/prompts/.*.eval.spec.ts --retryTimes=1",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate:latest": "knex migrate:latest --knexfile=src/db/knexfile.ts",
    "db:migrate:rollback": "knex migrate:rollback --knexfile=src/db/knexfile.ts",
    "postinstall": "prisma generate",
    "smee": "npx smee-client",
    "prepare": "husky"
  },
